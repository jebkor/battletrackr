{
  "name": "battletrackr",
  "version": "1.0.0-beta",
  "description": "",
  "main": "webpack.config.js",
  "browserslist": [
    "last 8 versions",
    "ie >= 9",
    "< 5%"
  ],
  "apiEndpointDev": "http://localhost:3000",
  "apiEndpointProd": "https://battletrackr-api-production.herokuapp.com",
  "scripts": {
    "start": "npm-run-all -p -r webpack:dev server",
    "start:build": "npm-run-all -p -r webpack:prod server",
    "webpack:dev": "SET API_ENDPOINT=%npm_package_apiEndpointDev%&& webpack --config webpack.dev.config.js --progress --watch",
    "webpack:prod": "SET API_ENDPOINT=%npm_package_apiEndpointProd%&& webpack --config webpack.prod.config.js --progress --watch",
    "server": "http-server -p 4000",
    "build": "SET API_ENDPOINT=%npm_package_apiEndpointProd%&& webpack --progress --config webpack.prod.config.js",
    "patch": "npm version patch",
    "analyze": "webpack --progress --config webpack.test.config.js"
  },
